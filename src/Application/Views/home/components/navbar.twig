{% if (contents == 'login') %}
    <nav class="navbar navbar-expand-lg bg-primary">
        <div class="container-fluid">
            <a class="navbar-brand align-items-center d-flex" href="login">
                <img src="assets/img/appImg.jpg" alt="Logo" class="d-inline-block align-text-top rounded-circle logo">
                {% trans %}AppName{% endtrans %}
            </a>
            <a href="signin">
                <button class="btn me-2 btn-primary" type="button">{% trans %}SignIn{% endtrans %}</button>
            </a>
        </div>
    </nav>
{% elseif (contents == 'signin') %}
    <nav class="navbar navbar-expand-lg bg-primary">
        <div class="container-fluid">
            <a class="navbar-brand align-items-center d-flex" href="signin">
                <img src="assets/img/appImg.jpg" alt="Logo" class="d-inline-block align-text-top rounded-circle logo">
                {% trans %}AppName{% endtrans %}
            </a>
            <a href="login">
                <button class="btn me-2 btn-primary" type="button">{% trans %}HomeLogin{% endtrans %}</button>
            </a>
        </div>
    </nav>
{% else %}
    <nav class="navbar navbar-expand-lg bg-primary">
        <div class="container-fluid">
            <a class="navbar-brand align-items-center d-flex" href="home">
                <img src="assets/img/appImg.jpg" alt="Logo" class="d-inline-block align-text-top rounded-circle logo">
                {% trans %}AppName{% endtrans %}
            </a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent"
                    aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="home-presentation">
                            <button class="btn me-2 btn-abs-nav" type="button">{% trans %}HomePresentation{% endtrans %}</button>
                        </a>
                    </li>
                </ul>

                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link active" href="login">
                            <button class="btn me-2 btn-abs-nav" type="button">{% trans %}HomeLogin{% endtrans %}</button>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" href="signin">
                            <button class="btn me-2 btn-primary" type="button">{% trans %}HomeSignIn{% endtrans %}</button>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
{% endif %}